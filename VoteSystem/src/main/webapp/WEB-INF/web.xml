<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <filter>
        <filter-name>CodeFilter</filter-name>
        <filter-class>filter.CharsetFilter</filter-class>
        <init-param>
            <param-name>encode</param-name>
            <param-value>utf-8</param-value>
        </init-param>
    </filter>
    <filter>
        <filter-name>isLoginFilter</filter-name>
        <filter-class>filter.isLoginFilter</filter-class>
    </filter>
    <filter>
        <filter-name>AdminFilter</filter-name>
        <filter-class>filter.AdminFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>AdminFilter</filter-name>
        <url-pattern>/manage_user.jsp</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>CodeFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>isLoginFilter</filter-name>
        <url-pattern>/main.jsp</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>isLoginFilter</filter-name>
        <url-pattern>/new_vote.jsp</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>isLoginFilter</filter-name>
        <url-pattern>/vote_result.jsp</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>isLoginFilter</filter-name>
        <url-pattern>/vote_oper.jsp</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>isLoginFilter</filter-name>
        <url-pattern>/addresult</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>isLoginFilter</filter-name>
        <url-pattern>/vote</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>isLoginFilter</filter-name>
        <url-pattern>/analyze</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>isLoginFilter</filter-name>
        <url-pattern>/getusers</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>isLoginFilter</filter-name>
        <url-pattern>/del</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>isLoginFilter</filter-name>
        <url-pattern>/getall</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>isLoginFilter</filter-name>
        <url-pattern>/update</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>isLoginFilter</filter-name>
        <url-pattern>/updateuser</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>CodeFilter</filter-name>
        <url-pattern>/search</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>servlet.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>servlet.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetAllVoteItemServlet</servlet-name>
        <servlet-class>servlet.GetAllVoteItemServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddVoteServlet</servlet-name>
        <servlet-class>servlet.AddVoteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetItemServlet</servlet-name>
        <servlet-class>servlet.GetItemServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateVoteItemServlet</servlet-name>
        <servlet-class>servlet.UpdateVoteItemServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DelVoteItemServlet</servlet-name>
        <servlet-class>servlet.DelVoteItemServlet</servlet-class>
    </servlet>
