        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetAllVoteItemServlet</servlet-name>
        <url-pattern>/getall</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddVoteServlet</servlet-name>
        <url-pattern>/addvote</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
