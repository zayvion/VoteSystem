        <url-pattern>/getdata</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateVoteItemServlet</servlet-name>
        <url-pattern>/update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DelVoteItemServlet</servlet-name>
        <url-pattern>/del</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowVoteResultServlet</servlet-name>
        <url-pattern>/result</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VoteServlet</servlet-name>
        <url-pattern>/vote</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddResultServlet</servlet-name>
        <url-pattern>/addresult</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AnalyzeDataServlet</servlet-name>
        <url-pattern>/analyze</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetAllUsersServlet</servlet-name>
        <url-pattern>/getusers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetUserServlet</servlet-name>
        <url-pattern>/getuser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdataUserServlet</servlet-name>
        <url-pattern>/updateuser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchServlet</servlet-name>
        <url-pattern>/search</url-pattern>
    </servlet-mapping>
