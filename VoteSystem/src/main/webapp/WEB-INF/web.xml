    </servlet>
    <servlet>
        <servlet-name>GetUserServlet</servlet-name>
        <servlet-class>servlet.GetUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdataUserServlet</servlet-name>
        <servlet-class>servlet.UpdataUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchServlet</servlet-name>
        <servlet-class>servlet.SearchServlet</servlet-class>
    </servlet>
